{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": false,
    "sourceMap": true,
    "moduleResolution": "node",
    "target": "es5",
    "module": "commonjs",
    "alwaysStrict": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strictNullChecks": false,
    "suppressImplicitAnyIndexErrors": true,
    "lib": [
      "es2016"
    ]
  },
  "exclude": [
    "**/*.spec.ts",
    "**/*.tests.ts",
    "test-setup.ts"
  ]
}
