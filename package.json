{
  "name": "cypher-query-builder",
  "version": "0.1.0-alpha.1",
  "description": "An intuitive, easy to use query builder for Neo4j and Cypher",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --progress",
    "build:prod": "webpack --progress --env.production",
    "test": "nyc --reporter=html --reporter=text mocha src/*.spec.js src/**/*.spec.js"
  },
  "keywords": [
    "cypher",
    "query",
    "builder",
    "neo4j"
  ],
  "author": "James Ferguson",
  "license": "MIT",
  "dependencies": {
    "@types/chai": "^4.0.4",
    "@types/lodash": "^4.14.77",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.33",
    "awesome-typescript-loader": "^3.2.3",
    "chai-as-promised": "^7.0.0",
    "lodash": "^4.17.4",
    "neo4j-driver": "^1.5.0",
    "node-cleanup": "^2.1.2",
    "typescript": "^2.5.3",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.6.0"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "mocha": "^3.4.2",
    "nyc": "^11.2.1"
  },
  "nyc": {
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/**/*.tests.js"
    ]
  }
}
